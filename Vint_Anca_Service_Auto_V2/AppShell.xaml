<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Vint_Anca_Service_Auto_V2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Vint_Anca_Service_Auto_V2"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            />
        <ShellContent
            Title="Mecanici"
            ContentTemplate="{DataTemplate local:MecanicPage}"
             />
        <ShellContent
            Title="Clienti"
            ContentTemplate="{DataTemplate local:ClientEntryPage}"
            />
        <ShellContent
            Title="Programari"
            ContentTemplate="{DataTemplate local:ProgramareEntryPage}"
        />

    </TabBar>

</Shell>